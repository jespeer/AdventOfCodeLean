-- Iterate through input, if instances of '(' minus instances of ')' is ever -1, return index
def basement (s : String) : Option Nat := Id.run do
  let mut count := 1
  for i in [:s.length] do
    let c := s.get (String.Pos.mk i)
    count := if c = '(' 
              then count + 1 
             else if c = ')' 
              then count - 1 else count
    if count = -1 then return i
  none

